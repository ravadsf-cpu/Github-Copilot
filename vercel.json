{
  "buildCommand": "cd news-app-new && npm install && npm run build",
  "installCommand": "cd news-app-new && npm install",
  "outputDirectory": "news-app-new/.next",
  "framework": "nextjs",
  "devCommand": "cd news-app-new && npm run dev",
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
